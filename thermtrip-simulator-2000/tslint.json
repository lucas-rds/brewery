{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "import-blacklist": [
            true,
            "rxjs/Rx"
        ],
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "eofline": true,
        "curly": true,
        "max-line-length": [
            true,
            140
        ],
    },
    "rulesDirectory": []
}